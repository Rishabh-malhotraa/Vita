{
  "name": "vita-backend",
  "version": "1.0.0",
  "description": "Vita app backend",
  "main": "dist/src/index.js",
  "proxy": "http://localhost:5000",
  "engines": {
    "npm": ">=8.1.0",
    "node": ">=16.13.0"
  },
  "scripts": {
    "build-ts": "tsc",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/src/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "pretty": "prettier --write \"./**/*.{ts,js}\"",
    "lint": "eslint \"**/*.{ts,tsx}\" --ignore-pattern node_modules/",
    "lint:fix": "eslint \"**/*.ts\" --ignore-pattern node_modules/ --fix",
    "pre-commit": "npm run pretty && npm run lint:fix"
  },
  "author": "Rishabh Malhotra",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@types/nodemailer": "^6.4.4",
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.10",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "ics": "^2.35.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "nanoid": "^3.1.30",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-twitter": "^0.1.5",
    "socket.io": "^3.0.5",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.7",
    "@types/passport": "^1.0.7",
    "@types/passport-github": "^1.1.6",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-linkedin-oauth2": "^1.5.3",
    "@types/passport-twitter": "^1.0.37",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-xo": "^0.39.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1"
  },
  "keywords": []
}